{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "constructor-super": 0,
    "no-case-declarations": 0,
    "no-class-assign": 0,
    "no-compare-neg-zero": 0,
    "no-cond-assign": 2,
    "no-console": [1, { "allow": ["warn", "error"] }],
    "no-const-assign": 2,
    "no-constant-condition": 0,
    "no-control-regex": 0,
    "no-debugger": 2,
    "no-delete-var": 0,
    "no-dupe-args": 0,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 0,
    "no-empty": 2,
    "no-empty-character-class": 0,
    "no-empty-pattern": 0,
    "no-ex-assign": 0,
    "no-extra-boolean-cast": 0,
    "no-extra-semi": 2,
    "no-fallthrough": 0,
    "no-func-assign": 0,
    "no-global-assign": 0,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-symbol": 0,
    "no-obj-calls": 0,
    "no-octal": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 0,
    "no-self-assign": 0,
    "no-sparse-arrays": 0,
    "no-this-before-super": 0,
    "no-undef": 0,
    "no-unexpected-multiline": 0,
    "no-unreachable": 2,
    "no-unsafe-finally": 0,
    "no-unsafe-negation": 0,
    "no-unused-labels": 0,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "no-useless-escape": 2,
    "require-yield": 0,
    "use-isnan": 0,
    "valid-typeof": 0,
    "valid-jsdoc": [0, {
      "requireParamDescription": false,
      "requireReturnDescription": false,
      "requireReturn": false,
      "prefer": {"returns": "return"}
    }],
    "curly": [0, "multi-line"],
    "guard-for-in": 0,
    "no-caller": 0,
    "no-extend-native": 2,
    "no-extra-bind": 0,
    "no-invalid-this": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 0,
    "no-new-wrappers": 0,
    "no-throw-literal": 0,
    "no-with": 2,
    "array-bracket-spacing": [2, "never"],
    "brace-style": 2,
    "camelcase": [2, {"properties": "never"}],
    "comma-dangle": [2, "only-multiline"],
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 0,
    "eol-last": 2,
    "func-call-spacing": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": "off",
    "new-cap": 2,
    "no-array-constructor": 0,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-new-object": 0,
    "no-trailing-spaces": [2, { "ignoreComments": false }],
    "object-curly-spacing": 2,
    "one-var": [0, {
      "var": "never",
      "let": "never",
      "const": "never"
    }],
    "padded-blocks": [0, "never"],
    "quote-props": [0, "consistent"],
    "quotes": [2, "single", {"allowTemplateLiterals": true}],
    "require-jsdoc": [0, {
      "require": {
        "FunctionDeclaration": true,
        "MethodDefinition": true,
        "ClassDeclaration": true
      }
    }],
    "semi-spacing": 2,
    "semi": 2,
    "space-infix-ops": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "spaced-comment": [0, "always"],
    "arrow-parens": [2, "always"],
    "generator-star-spacing": [0, "after"],
    "no-var": 2,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "rest-spread-spacing": 0,
    "yield-star-spacing": [0, "after"],
    "max-len": [2, 120, 2, {
      "ignoreUrls": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
    }],
    "indent": [2, 2],
    "no-whitespace-before-property": 2,
    "eqeqeq": 2,
    "lines-between-class-members": [2, "always"]
  },
  "settings": {
    "react": {
      "version": "16.8.6"
    }
  }
}
